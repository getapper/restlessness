{
  "service": "restlessness-rest-api-shared-resources",
  "provider": {
    "name": "aws",
    "runtime": "nodejs12.x"
  },
  "resources": {
    "Resources": {
      "SharedGW": {
        "Type": "AWS::ApiGateway::RestApi",
        "Properties": {
          "Name": "SharedGW"
        }
      }
    },
    "Outputs": {
      "apiGatewayRestApiId": {
        "Value": {
          "Ref": "SharedGW"
        },
        "Export": {
          "Name": "rln-backend-SharedGW-restApiId"
        }
      },
      "apiGatewayRestApiRootResourceId": {
        "Value": {
          "Fn::GetAtt": [
            "SharedGW",
            "RootResourceId"
          ]
        },
        "Export": {
          "Name": "rln-backend-SharedGW-rootResourceId"
        }
      }
    }
  }
}