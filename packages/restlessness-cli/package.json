{
  "name": "@restlessness/cli",
  "version": "0.0.0",
  "description": "",
  "main": "dist/index.js",
  "scripts": {
    "build-backend": "cd rln-backend && tsc",
    "build-frontend": "cd rln-frontend && npm run build",
    "build": "npm run tsc && npm run build-backend && npm run build-frontend && node scripts/copy-files.js",
    "tsc": "rimraf dist && tsc -p tsconfig.json",
    "prepublishOnly": "npm run build",
    "npm:tag": "node ../../scripts/git-packages-tagger.js",
    "update-deps": "npm i @restlessness/core@latest -S -E && cd rln-backend && npm run update-deps"
  },
  "bin": {
    "restlessness": "./dist/index.js"
  },
  "keywords": [
    "restlessness",
    "cli",
    "lambda",
    "serverless",
    "typescript"
  ],
  "author": "Getapper",
  "license": "ISC",
  "dependencies": {
    "@restlessness/core": "file:../restlessness-core",
    "chalk": "3.0.0",
    "minimist": "1.2.3",
    "module-alias": "2.2.0",
    "serverless-offline": "5.12.1",
    "shelljs": "0.8.4",
    "yup": "0.28.3"
  },
  "devDependencies": {
    "@types/chalk": "2.2.0",
    "@types/minimist": "1.2.0",
    "@types/node": "12.12.45",
    "@typescript-eslint/parser": "3.2.0",
    "eslint": "^6.6.0",
    "rimraf": "3.0.2",
    "typescript": "3.7.5"
  },
  "peerDependencies": {
    "serve": "^11.3.0",
    "serverless": "^1.63.0"
  },
  "_moduleAliases": {
    "root": "lib/assets/backend/dist"
  }
}
